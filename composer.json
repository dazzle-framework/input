{
    "name": "dazzlesoftware/input",
    "type": "library",
    "description": "Dazzle Input Package",
    "keywords": ["dazzle", "framework", "input"],
    "homepage": "https://github.com/dazzle-framework/input",
    "license": "GPL-3.0-or-later",
    "require": {
       "php": "^7.3.0",
        "dazzlesoftware/filter": "^1.0|^2.0"
    },
    "require-dev": {
        "dazzlesoftware/dazzle-coding-standard": "^1.0|^2.0",
        "dazzlesoftware/test": "^2.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\Input\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\Input\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-develop": "2.x-dev"
        }
    }
}
